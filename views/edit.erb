									            </div>
									          </div>
							
							
							
						</td>
						
						<td>
							
							<div class="control-group">
							            <label class="control-label" for="dob">Date Of Birth</label>
							            <div class="controls">
							              	<select class="input-small" name="student[m]">
								
									<option> - Month - </option>
									
											<% 
													months = ["January", "February", "March", "April", "May", "June", "August", "September", "October", "November", "December"]
													months.each do |m|
														
														
											%>
											
													<% if m == @s.m %>
														<option value="<%=m %>" selected><%=m%></option>
														<% else %>
											
													<option value="<%=m %>"><%=m%></option>
													<% end %>
											<% end %>
											</select>

											<select class="input-small" name="student[d]">
												<option> - Day - </option>
												<%
													day = 1
													31.times do 
														
														
												%>
												
													<% if day.to_i == @s.d.to_i %>
														<option value="<%=day%>" selected><%=day%></option>
														<% else %>
											
													<option value="<%=day%>"><%=day%></option>
													<% end %>
													
												
												<%
													day = day + 1
													end
												%>
											</select>

											<select class="input-small" name="student[y]">
												<option> - Year - </option>
												<%
													year = 1970
													
													30.times do
												%>
												
													<% if year == @s.y.to_i %>
													<option value="<%=year%>" selected="selected"><%=year%></option>
														<% else %>
											
													<option value="<%=year%>"><%=year%></option>
													<% end %>
													
												
													
													
												<%
													year = year + 1
												end
												%>
												
											</select>
							            </div>
							          </div>



									 <div class="control-group">
									            <label class="control-label">Gender</label>
